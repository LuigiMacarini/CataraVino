<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CataraVinhos"
        tools:targetApi="31">


        <activity
            android:name=".DashboardActivity"
            android:exported="true"
            tools:ignore="MissingClass">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Historia"/>
        <activity android:name=".WinesActivity" />
        <activity android:name=".GenresActivity" />
        <activity android:name=".ContactActivity" />
        <activity android:name=".Info" />
        <activity android:name=".CartActivity" />
        <activity android:name=".Home" />
        <activity android:name=".AddVinhoActivity"/>
        <activity android:name=".CatalogoActivity"/>
        <activity android:name=".CadastroActivity"/>
        <activity android:name=".CadastroPedidoActivity"/>
        <activity android:name=".ConsultaPedidoActivity"/>
        <activity android:name=".ListaClientesActivity"/>
        <activity android:name=".ConsultaComissaoActivity"/>
        <activity android:name=".ListaVinhosActivity"/>


    </application>

</manifest>
